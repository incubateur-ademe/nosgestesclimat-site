{
	"name": "nosgestesclimat-site",
	"license": "MIT",
	"version": "3.2.0",
	"description": "The leading open source climate footprint calculator",
	"repository": {
		"type": "git",
		"url": "https://github.com/datagir/nosgestesclimat-site.git"
	},
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"model:compile": "yarn --cwd ./nosgestesclimat/ compile",
		"model:compile:fr": "yarn --cwd ./nosgestesclimat/ compile:fr",
		"model:rules-watch": "yarn --cwd ./nosgestesclimat/ compile:rules-watch",
		"serve": "webpack serve --mode development --config webpack.dev.js",
		"start": "yarn run serve & yarn run model:watch",
		"dev": "yarn run model:compile:fr && yarn run serve",
		"compile": "NODE_ENV=production yarn run webpack --config webpack.prod.js",
		"stats": "webpack --config webpack.prod.js --profile --json > stats.json",
		"e2e": "yarn run e2e:generate:personas && cypress open",
		"test": "yarn run e2e:generate:personas && cypress run",
		"e2e:generate:personas": "node cypress/scripts/generateSpecsFromPersonas.js",
		"analyze-bundle": "ANALYZE_BUNDLE=1 yarn run build",
		"clean": "rimraf dist node_modules",
		"test:bundlesize": "bundlesize",
		"generate:ui": "node scripts/i18n/generate-ui.js",
		"translate:ui": "node scripts/i18n/translate-ui.js",
		"translate:faq": "node scripts/i18n/translate-faq.js",
		"check:faq": "node scripts/i18n/check-faq.js",
		"check:ui": "node scripts/i18n/generate-ui.js && node scripts/i18n/check-ui.js",
		"translate:pages": "node scripts/i18n/translate-pages.js",
		"translate:releases": "node scripts/i18n/translate-release.js",
		"check:releases": "ts-node scripts/fetch-releases.ts",
		"illustration": "cat source/images/illustration.svg | sed -e 's/inkscape:label/class/g; s/#6861b3/var(--color)/g' | svgr --ref > source/components/IllustrationSVG.tsx",
		"lint": "eslint . --ext .js,.ts,.tsx",
		"lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
		"sitemap": "ts-node ./scripts/generateSitemap.ts "
	},
	"browserslist": [
		"> 1% in FR"
	],
